<div class="table">
<table>
  <thead>
    <tr>
      {{#each data.columns}}
        <th>{{this}}</th>
      {{/each}}
      {{#if abm}}
        <th>Acci√≥n</th>
      {{/if}}
    </tr>
  </thead>
  <tbody>
    {{#each data.values}}
      <tr id={{this.id}}>
        {{#each this}}
          {{#ifNotEq @key "id"}}
            <td>{{this}}</td>
          {{/ifNotEq}}
        {{/each}}
        {{#if ../abm}}
          <td>
            <button class="addProduct">Agregar</button>
            <button class="deleteProduct">Borrar</button>
          </td>
        {{/if}}
      </tr>
    {{/each}}
  </tbody>
</table>
</div>
